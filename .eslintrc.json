{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"require": "readonly",
		"module": "readonly",
		"OO": "readonly",
		"L": "readonly"
	},
	"rules": {
		"max-len": "off",
		"mediawiki/class-doc": "warn",
		"mediawiki/valid-package-file-require": "warn",
		"prefer-regex-literals": "warn"
	}
}
