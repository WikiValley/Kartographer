{
	"extends": "wikimedia",
	"env": {
		"browser": true,
		"jquery": true,
		"qunit": true
	},
	"globals": {
		"ve": true,
		"mw": true,
		"mediaWiki": false,
		"OO": false,
		"L": false
	},
	"rules": {
		"dot-notation": 0,
		"wrap-iife": 0,
		"no-console": "error",
		"spaced-comment": ["error", "always", { "exceptions": ["*", "!"] }],
		"vars-on-top": "error"
	}
}
