<h2>{{LBL_EXTERNAL_SERVICES}}</h2>

<table>
    <thead>
    <tr>
        <th>{{LBL_SERVICE}}</th>
        <td></td>
    </tr>
    </thead>
    <tbody>
    {{#services}}
    <tr{{#featured}} class="featured"{{/featured}}>
        <th>{{name}}
        </th>
        <td>
            <a rel="nofollow" class="external free" href="{{link}}" data-service="{{id}}" target="_blank">{{linkLabel}}</a>
        </td>
    </tr>
    {{/services}}
    </tbody>
</table>
