{
	"name": "Kartographer",
	"author": [
		"Yuri Astrakhan"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Kartographer",
	"descriptionmsg": "kartographer-desc",
	"license-name": "MIT",
	"type": "other",
	"MessagesDirs": {
		"Kartographer": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"Kartographer\\Singleton": "includes/Kartographer.hooks.php"
	},
	"ResourceModules": {
		"ext.kartographer.error": {
			"styles": [
				"styles/error.less"
			],
			"targets": [
				"mobile",
				"desktop"
			]
		},
		"ext.kartographer.static": {
			"styles": [
			  "styles/static.less"
			],
			"targets": [
				"mobile",
				"desktop"
			]
		},
		"ext.kartographer.live": {
			"scripts": [
			  "lib/mapbox-lib.js",
			  "modules/kartographer.js"
			],
			"styles": [
			  "styles/live.less",
			  "lib/style.css"
			],
			"targets": [
				"mobile",
				"desktop"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "Kartographer"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"Kartographer\\Singleton::onParserFirstCallInit"
		],
		"ParserAfterParse": [
			"Kartographer\\Singleton::onParserAfterParse"
		],
		"UnitTestsList": [
			"Kartographer\\Singleton::onUnitTestsList"
		]
	},
	"config": {
		"KartographerDfltStyle": "osm-intl",
		"KartographerStyles": ["osm-intl", "osm"],
		"KartographerSrcsetScales": [1.3, 1.5, 2, 2.6, 3],
		"KartographerMapServer": "https://maps.wikimedia.org",
		"KartographerIconServer": "https://maps.wikimedia.org"
	},
	"TrackingCategories": [
		"kartographer-tracking-category",
		"kartographer-broken-category"
	],
	"manifest_version": 1
}
